{
	"name": "datalake_dbsql",
	"properties": {
		"activities": [
			{
				"name": "copia_prueba_datalake_bd",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "if exists (select 1 from sys.tables t  where schema_name(t.schema_id)='dbo' and t.name ='csv_bdsql') truncate table dbo.csv_bdsql",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"dataIntegrationUnits": 2,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "prueba_csv_2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "csv_bdsql",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}